{
  "name": "chargify",
  "version": "0.0.0",
  "description": "Chargify API client",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@types/node-fetch": "2.1.4",
    "node-fetch": "2.3.0",
    "url": "0.11.0"
  },
  "devDependencies": {
    "ava": "1.1.0",
    "npm-run-all": "4.1.5",
    "typescript": "3.2.4"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "tsc -p tsconfig.build.json --outDir lib",
    "build:watch": "tsc -p tsconfig.build.json --outDir lib -w",
    "unit": "ava lib/index.spec.js",
    "unit:watch": "ava lib/index.spec.js -w",
    "test": "run-s build unit",
    "test:watch": "run-p build:watch unit:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyrusBiotechnology/chargify.git"
  },
  "keywords": [
    "chargify"
  ]
}
